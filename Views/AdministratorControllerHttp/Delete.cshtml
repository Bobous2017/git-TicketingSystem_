@model TicketingSystemLibrary.Models.Administrator



<div class="styled">
    <h1>Delete Administrator</h1>
    <h4>Are you sure you want to delete this Administrator?</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ID
        </dt>
        <dd class="col-sm-10">
            @Model.AdminId
        </dd>
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Model.Name
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Model.Email
        </dd>
    </dl>

    <form asp-action="DeleteConfirmed" method="post">
        <!-- Notice the change here -->
        <input type="hidden" name="id" value="@Model.AdminId" />
        <button type="submit" class="btn btn-danger">Delete</button>
        <a asp-action="Index" class="btn btn-secondary">Cancel</a>
    </form>
</div>
