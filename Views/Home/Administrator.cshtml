@model List<TicketingSystemLibrary.Models.Administrator>

<h1>Administrator List</h1>

@if (Model == null || !Model.Any())
{
    <p>No administrators found.</p>
}
else
{
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var admin in Model)
            {
                <tr>
                    <td>@admin.Id</td>
                    <td>@admin.Name</td>
                    <td>@admin.Email</td>
                </tr>
            }
        </tbody>
    </table>
